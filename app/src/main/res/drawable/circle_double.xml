<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Larger blue circle_primary_stroke_white in back -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <padding
                android:top="2dp"
                android:left="2dp"
                android:right="2dp"
                android:bottom="2dp">
            </padding>
            <size
                android:width="24dp"
                android:height="24dp"/>
        </shape>
    </item>
    <!-- Smaller red circle_primary_stroke_white in front -->
    <item>
        <shape
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:shape="oval">
            <!-- transparent stroke = larger_circle_size - smaller_circle_size -->
            <stroke
                android:width="16dp"
                android:color="@color/primaryColor" />
            <solid android:color="@color/white" />
            <size
                android:width="8dp"
                android:height="8dp" />
        </shape>
    </item>
</layer-list>